# Qdrant 配置
QDRANT_HOST=localhost
QDRANT_PORT=6333
QDRANT_GRPC_PORT=6334
QDRANT_COLLECTION_NAME=metrics
# QDRANT_API_KEY=your_api_key_here  # 如果需要认证

# 向量化器配置
VECTORIZER_MODEL_NAME=m3e-base
VECTORIZER_DEVICE=cpu
VECTORIZER_BATCH_SIZE=32

# Neo4j 配置
NEO4J_URI=bolt://localhost:7687
NEO4J_USER=neo4j
NEO4J_PASSWORD=password

# 智谱 AI 配置（用于 GLM 摘要生成）
ZHIPUAI_API_KEY=your_zhipuai_api_key_here
ZHIPUAI_MODEL=glm-4-flash
ZHIPUAI_BATCH_SIZE=5

# 应用配置
APP_NAME=Semantic Query System
DEBUG=false
LOG_LEVEL=INFO
